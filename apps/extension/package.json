  "scripts": {
    "start": "rm -rf dist && webpack --config webpack/webpack.dev.js --watch",
    "dev": "rm -rf dist && webpack --config webpack/webpack.dev.js --watch",
    "update:translations": "yarn i18next --silent --config i18next-parser.config.cjs",
    "build": "rm -rf dist && webpack --config webpack/webpack.prod.js --env build=qa",
    "build:extension": "yarn build",
    "build:extension:ci": "rm -rf dist && webpack --config webpack/webpack.prod.js --env build=ci",
    "build:extension:canary": "rm -rf dist && webpack --config webpack/webpack.prod.js --env build=canary",
    "build:extension:prod": "rm -rf dist && webpack --config webpack/webpack.prod.js --env build=production",
    "test": "PORT_PREFIX=talisman EXTENSION_PREFIX=\"\" jest --maxWorkers=50% --collectCoverage",
    "test:watch": "PORT_PREFIX=talisman EXTENSION_PREFIX=\"\" jest --watch",
    "lint": "eslint . --max-warnings 0 --ext ts,tsx",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "clean": "rm -rf dist && rm -rf .turbo && rm -rf node_modules"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
