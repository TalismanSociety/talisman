    "@talismn/balances": "workspace:*",
    "@talismn/util": "workspace:*",
    "bcryptjs": "^2.4.3",
    "blueimp-md5": "2.19.0",
    "dexie": "^3.2.4",
    "extension-shared": "workspace:*",
    "i18next": "^22.5.0",
    "rxjs": "^7.8.1",
    "semver": "^7.5.4",
    "ts-results": "3.3.0",
